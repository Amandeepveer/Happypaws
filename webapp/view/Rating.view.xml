<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.team25HappyPaws.controller.Rating" xmlns="sap.m" xmlns:layout="sap.ui.commons.layout">
    <Page showHeader="false" title="Page Title" showFooter="false" showNavButton="false" enableScrolling="false">
        <content>
            <layout:AbsoluteLayout horizontalScrolling="Hidden" verticalScrolling="Auto">
                <layout:positions>
                    <layout:PositionContainer left="18px" top="181px">
                        <layout:control>
                            <RatingIndicator value="0" maxValue="5" visualMode="Full" enabled="true" visible="true" editable="true"/>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="98px" top="278px">
                        <layout:control>
                            <ToggleButton pressed="true" text="RATE US" iconFirst="true" width="auto" enabled="true" visible="true" press="_onToggleButtonPress"/>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="73px" top="1px">
                        <layout:control>
                            <Image src="/webapp/resources/happypaws.png" height="154px" width="154px" visible="true" densityAware="false"/>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="199px" top="450px">
                        <layout:control>
                            <ToggleButton pressed="true" text="NEXT" iconFirst="true" width="auto" enabled="true" visible="true" press="_onToggleButtonPress1"/>
                        </layout:control>
                    </layout:PositionContainer>
                </layout:positions>
            </layout:AbsoluteLayout>
        </content>
        <footer>
            <OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
                <content>
                    <ToolbarSpacer width=""/>
                    <Button text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                    <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                </content>
            </OverflowToolbar>
        </footer>
        <headerContent/>
        <subHeader/>
        <customHeader/>
    </Page>
</mvc:View>
