                    </layout:PositionContainer>
                    <layout:PositionContainer left="13px" top="477px">
                        <layout:control>
                            <ToggleButton pressed="true" text="BACK" iconFirst="true" width="auto" enabled="true" visible="true" press="_onToggleButtonPress1"/>
