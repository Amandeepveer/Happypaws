                                    <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
                                </form:layout>
                            </form:Form>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="0px" top="189px">
                        <layout:control>
                            <form:Form width="100%" editable="true" visible="true">
                                <form:title>
                                    <core:Title text="DROP OFF" level="Auto"/>
                                </form:title>
                                <form:formContainers/>
                                <form:layout>
                                    <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
                                </form:layout>
                            </form:Form>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="256px" top="479px">
                        <layout:control>
                            <ToggleButton pressed="true" text="NEXT" iconFirst="true" width="auto" enabled="true" visible="true" press="_onToggleButtonPress"/>
