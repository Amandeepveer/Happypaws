                                    <Menu title="TYPES">
                                        <items>
                                            <MenuItem text="SMALL DOG" enabled="true" icon="sap-icon://edit" visible="true" startsSection="false">
                                                <items/>
                                            </MenuItem>
                                            <MenuItem text="LARGE DOG" enabled="true" icon="sap-icon://save" visible="true" startsSection="false" press="_onMenuItemPress">
                                                <items/>
                                            </MenuItem>
                                            <MenuItem text="PUPPIES" enabled="true" icon="sap-icon://open-folder" visible="true" startsSection="false" press="_onMenuItemPress1">
                                                <items/>
                                            </MenuItem>
                                        </items>
                                    </Menu>
                                </menu>
                            </MenuButton>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="0px" top="145px">
