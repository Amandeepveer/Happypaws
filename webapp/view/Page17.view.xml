<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.team25HappyPaws.controller.Page17" xmlns="sap.m" xmlns:vbm="sap.ui.vbm" xmlns:layout="sap.ui.commons.layout">
    <Page showHeader="false" title="Page Title" showFooter="false" showNavButton="false" enableScrolling="false" navButtonPress="_onPageNavButtonPress">
        <content>
            <layout:AbsoluteLayout horizontalScrolling="Hidden" verticalScrolling="Auto">
                <layout:positions>
                    <layout:PositionContainer left="0px" top="120px">
                        <layout:control>
                            <vbm:GeoMap legendVisible="true" scaleVisible="true" width="100%" height="226px" initialPosition="151.008;-33.8205;500" centerPosition="" initialZoom="5" zoomlevel="15" enableAnimation="false" refMapLayerStack="Default" disableZoom="false" disablePan="false">
                                <vbm:resources/>
                                <vbm:legend/>
                                <vbm:vos>
                                    <vbm:Spots items="{path:'/PETSITSet', templateShareable:true}">
                                        <vbm:items>
                                            <vbm:Spot position="{ID}" text="{Rangepets}" image="{dropoff}" alignment="5"/>
                                        </vbm:items>
                                    </vbm:Spots>
                                    <vbm:Circles>
                                        <vbm:items/>
                                    </vbm:Circles>
                                    <vbm:Routes>
                                        <vbm:items/>
                                    </vbm:Routes>
                                </vbm:vos>
                            </vbm:GeoMap>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="0px" top="0px">
                        <layout:control>
                            <List mode="SingleSelectMaster" width="100%" headerText="MAP COMPONOMENT" footerText="" backgroundDesign="Solid" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true" visible="true">
                                <infoToolbar>
                                    <Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
                                        <content>
                                            <Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </content>
                                    </Toolbar>
                                </infoToolbar>
                                <headerToolbar/>
                                <items>
                                    <ObjectListItem intro="" title="" number="MAP TO FIND THE LOCATION" numberUnit="" numberState="None" type="Active" selected="true" showMarkers="false">
                                        <attributes/>
                                        <firstStatus/>
                                        <secondStatus/>
                                    </ObjectListItem>
                                </items>
                            </List>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="7px" top="378px">
                        <layout:control>
                            <Button text="BACK" type="Default" icon="sap-icon://nav-back" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
                        </layout:control>
                    </layout:PositionContainer>
                </layout:positions>
            </layout:AbsoluteLayout>
        </content>
        <footer>
            <OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
                <content>
                    <ToolbarSpacer width=""/>
                    <Button text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                    <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                </content>
            </OverflowToolbar>
        </footer>
        <headerContent/>
        <subHeader/>
        <customHeader/>
    </Page>
</mvc:View>
