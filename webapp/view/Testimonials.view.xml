<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.team25HappyPaws.controller.Testimonials" xmlns="sap.m" xmlns:layout="sap.ui.commons.layout">
    <Page showHeader="false" title="Page Title" showFooter="false" showNavButton="false" enableScrolling="false">
        <content>
            <layout:AbsoluteLayout horizontalScrolling="Hidden" verticalScrolling="Auto">
                <layout:positions>
                    <layout:PositionContainer left="1px" top="467px">
                        <layout:control>
                            <MessageStrip text="VIDEOS AND TESTIMONIALS COMMING SOON" type="Information" customIcon="" showIcon="true" showCloseButton="false" visible="true">
                                <link/>
                            </MessageStrip>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="192px" top="12px">
                        <layout:control>
                            <IconTabBar expandable="false" expanded="true" applyContentPadding="false" backgroundDesign="Solid" upperCase="false" visible="true" selectedKey="Ok">
                                <items>
                                    <IconTabFilter icon="sap-icon://video" iconColor="Neutral" text="" count="" design="Vertical" showAll="false" textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
                                        <content/>
                                    </IconTabFilter>
                                    <IconTabSeparator icon="" iconDensityAware="false"/>
                                </items>
                                <content/>
                            </IconTabBar>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="60px" top="112px">
                        <layout:control>
                            <Image src="/webapp/resources/happypaws.png" height="235px" width="235px" visible="true" densityAware="false"/>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="0px" top="302px">
                        <layout:control>
                            <Image src="/webapp/resources/Coming-Soon.png" height="137.14px" width="320px" visible="true" densityAware="false"/>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="25px" top="41px">
                        <layout:control>
                            <Button text="BACK" type="Default" icon="sap-icon://nav-back" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
                        </layout:control>
                    </layout:PositionContainer>
                </layout:positions>
            </layout:AbsoluteLayout>
        </content>
        <footer>
            <OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
                <content>
                    <ToolbarSpacer width=""/>
                    <Button text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                    <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                </content>
            </OverflowToolbar>
        </footer>
        <headerContent/>
        <subHeader/>
        <customHeader/>
    </Page>
</mvc:View>
